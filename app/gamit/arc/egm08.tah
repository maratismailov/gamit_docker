      Subroutine EGM08

c     Initialize constants according to IERS-2010 conventions
c     T. Herring 2003/R. King Dec 2010

      implicit none

      include '../includes/dimpar.h'   
      include '../includes/arc.h'


* LOCAL VARIABLES
      real*8 dyr_2000  ! Years since J2000 ref for gravity coefficients

      integer*4 i, i1
                              
      do i=1,11
        czhar(i)=0.d0
      enddo
      do i=1,77
        cchar(i)=0.d0
        cshar(i)=0.d0
      enddo

c        IERS Standards for Constants - coded from IERS Conventions 2010

      ltvel= 299792.458D0
      aunit= 1.495978707D8
      ertrad= 6378.13630 
      monrad = 1738.d0        ! not updated
      sunrad= 696000.D+00     ! not updated
C               GM OF THE EARTH
      gm(1)= 398600.4415D+0
cold  gm(1)= 398600.4418D+0    

C               GM OF THE MOON -not updated
      gm(2)= 4902.7989D+0
C               GM OF THE SUN              
      gm(3)= 1.32712442099D+11
C
      crad= ertrad

c-------------------------------------------------------------------------
c         ** former MIT code - effectively equivalent
c         LTVEL=299792.458D0
c         AULTSC=499.00478370D0
c         ERTRAD=6378.137D0
c         SUNRAD=6.96D+05
c         CONST=(0.01720209895D0)**2*(LTVEL)**3*(AULTSC)**3/(86400.D0)**2
c               GM OF THE EARTH
c         Value used until 9103012: SEM = 328900.553  => GM = 398600.4445
c              (Proper MERIT value: SEM = 328900.5501 << GM = 398600.448)
c         New value from CSR8901  : SEM = 328900.5564 << GM = 398600.4401
c         IERS/IGS 1992 stds value: SEM = 328900.5552 << GM = 398600.4418
c         GM(1)= CONST*81.3005883D0/(1.D0+81.3005883D0)*1.D0/328900.55523496d0
c               GM OF THE MOON
c         GM(2) = GM(1)/81.3005883D0
c               GM OF THE SUN
c         GM(3) = (GM(1)+GM(2))*328900.55523496d0
c----------------------------------------------------------------------------

C         Gravity field is EGM-2008  ('tide-free', i.e. permanent tide not included in C20) 

c         Values from file  EGM2008_to2190_TideFree downloaded from 
c         http://earth-info.nga.mil/GandG/wgs84/gravitymod/egm2008/egm08_wgs84.htm
c         (These should be checked against an IERS publication since C20, at least,
c         is discrepant)

c         scaling between 2-body GM and GM for GEM-T3   ! not updated
      crad = ertrad

****  Compute time in years since J2000    
*     use start time rather than IC epoch since g-file not yet read
c     dyr_2000 = (jde - 2451544.d0)/365.25d0                       
      dyr_2000 = (jd0 - 2451544.d0)/365.25d0                       


C     Zonals  C20, etc.              
               
C 
c      EGM08 is a "conventional tide-free" field, so set the flag to not remove
c      the permanent part of the tide in sbfn.f
       zero_tide = .false.           
c      Note: The Table 6.2 and the comments in Section 6.2 of the IERS Conventions indicate that
c            the tide-free value of C20 at epoch 2000 is -484.16531d-06.  Why the discrepancy?
c     czhar(1)= -484.165143790815D-06    + 11.6D-12 *dyr_2000   
* MOD TAH 180101: Value above is EGM08 tide free value based on 4-yrs of GRACE.  IERS2010 
*     from Table 6.2 is SLR 11-year value zero tide (-484.16948e-6 + 2 in last digit).  The
*     zero tide to tide free adjusment is -4.17e-9, bringing value to -484.16531e-6
      czhar(1)= -484.16531D-06    + 11.6D-12 *dyr_2000   

      czhar(2)=    0.957161207093473D-06 +  4.9D-12 *dyr_2000
      czhar(3)=    0.539965866638991D-06 +  4.7D-12 *dyr_2000  
      czhar(4)=    0.686702913736681D-07 
      czhar(5)=   -0.149953927978527D-06 
      czhar(6)=    0.905120844521618D-07  
      czhar(7)=    0.494756003005199D-07 
      czhar(8)=    0.280180753216300D-07   
      czhar(9)=    0.533304381729473D-07  
c RWK  These last two commented out for debugging EGM08, but comments not removed until 170310
      czhar(10)=  -0.507683787085927D-07  
      czhar(11)=   0.364361922614572D-07 
                        
