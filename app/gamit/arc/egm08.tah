C     Sectorials and Tesserals
          
c        (2,2) 

      cchar(2)=   0.243938357328313D-05  
      cshar(2)=  -0.140027370385934D-05  
  
C     (2,1) from Eq 6.5 in IERS Conventions 2010 to get mean figure axis aligned with IERS mean pole
        
c     get the mean pole at epoch 2000.0 from IERS Conventions 2010   
      if( jd0.lt.2455197.5000 ) then
c       prior to 2010.0  
        xpm = 0.055974d0 + 0.0018243d0*dyr_2000 + 0.00018413*dyr_2000**2
     .         + 0.000007024d0*dyr_2000**3 
        ypm = 0.346346d0 + 0.0017896d0*dyr_2000 - 0.00010729*dyr_2000**2
     .         - 0.000000908d0*dyr_2000**3 
cd        print *,'before 2010.0 dyr_2000 xpm ypm ',dyr_2000,xpm,ypm
      else
c       after 2010.0              
        xpm = 0.023513d0 + 0.0076141d0*dyr_2000 
        ypm = 0.358891d0 - 0.0006287d0*dyr_2000   
cd        print *,'after 2010.0 dyr_2000 xpm ypm ',dyr_2000,xpm,ypm
      endif 
c     Convert pole position to radians
      xpm = xpm*casr
      ypm = ypm*casr
      cchar(1) =  dsqrt(3.d0)*xpm*czhar(1) - xpm*cchar(2) + ypm*cshar(2)
      cshar(1) = -dsqrt(3.d0)*ypm*czhar(1) - ypm*cchar(2) - xpm*cshar(2)
         
